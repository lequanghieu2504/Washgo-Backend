{
  "services": {
    "backend": {
      "source": "./Washgo",
      "build": {
        "command": "mvn clean package -DskipTests"
      },
      "start": {
        "command": "java -jar target/washgo-0.0.1-SNAPSHOT.jar"
      }
    },
    "frontend": {
      "source": "./washgo-Frontend",
      "build": {
        "command": "npm install && npm run build"
      },
      "start": {
        "command": "npx serve -s dist -l 3000"
      }
    }
  }
}